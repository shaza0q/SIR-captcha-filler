{
  "manifest_version": 3,
  "name": "Auto Captcha Filler + Auto Submit",
  "version": "1.0",
  "description": "Automatically fills and submits the captcha for WebForms download links.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Auto Captcha"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
